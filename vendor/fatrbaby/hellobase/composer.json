{
    "name": "fatrbaby/hellobase",
    "description": "Hbase client php wrapper, via thirft",
    "keywords": [
        "hbase",
        "hadoop",
        "thirft",
        "big data",
        "fatrbaby"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fatrbaby",
            "email": "fatrbaby@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*.*"
    },
    "autoload": {
        "psr-4": {
            "HelloBase\\": "src/HelloBase",
            "Thrift\\": "bridge/Thrift"
        },
        "classmap": [
            "bridge/Hbase"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
